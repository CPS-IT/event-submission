# Event Submission Form
# Displays a form for event submission
#
# CType: event_submission_form

tt_content.event_submission_form =< styles.content.get
tt_content.event_submission_form {
    content.select {
        where.field = colPos
        where.wrap = colPos = |
    }
    templateName = EventSubmissionForm
    dataProcessing {
        20 = Fr\FrUtility\DataProcessor\FlexFormDataProcessor
        20 {
            as = flexform
            valuePath = settings
        }
    }
}
